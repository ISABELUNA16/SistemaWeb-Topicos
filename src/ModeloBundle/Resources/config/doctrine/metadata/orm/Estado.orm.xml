<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Estado" table="Estado">
    <id name="codEstado" type="integer" column="cod_estado">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="estDecripcion" type="string" column="est_decripcion" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estEstado" type="integer" column="est_estado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
