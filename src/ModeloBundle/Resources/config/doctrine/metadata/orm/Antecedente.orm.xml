<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Antecedente" table="Antecedente">
    <id name="codAntecedente" type="integer" column="cod_antecedente">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codper" type="integer" column="codper" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="antDescripcion" type="text" column="ant_descripcion" length="-1" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codTantecedente" type="integer" column="cod_tantecedente" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="antFechaReg" type="datetime" column="ant_fecha_reg" nullable="true"/>
    <field name="codUser" type="integer" column="cod_user" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="antEstado" type="integer" column="ant_estado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
