<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Medicamento" table="Medicamento">
    <id name="codMedicamento" type="integer" column="cod_medicamento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="medDescripcion" type="string" column="med_descripcion" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="medEstado" type="integer" column="med_estado" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
